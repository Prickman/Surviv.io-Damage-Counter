{
  "manifest_version": 2,
  "name": "Surviv.io Damage Counter",
  "version": "0.1",
  "description": "Surviv.io constantly updating damage counter",
  "browser_action": {
    "default_icon": "radioactive.png"
    "default_popup": "popup.html"
  },
  "content_scripts": [
      {
        "matches": [
          "*://surviv.io/*"
        ],
        "js": ["jquery-3.3.1.min.js", "content.js"]
      }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
